<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<?import java.net.URL?>
<AnchorPane fx:id="pane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.BoardMenu.BoardMenuSecondPageForLeaderView">
   <children>
      <Button fx:id="remove" layoutX="425.0" layoutY="39.0" mnemonicParsing="false" onAction="#removeBoard" prefHeight="26.0" prefWidth="138.0" text="remove board" />
      <Label fx:id="response" layoutX="145.0" layoutY="14.0" prefHeight="18.0" prefWidth="440.0" />
      <Button layoutX="549.0" layoutY="359.0" mnemonicParsing="false" onAction="#back" text="back" />
      <Label fx:id="condition" layoutX="14.0" layoutY="354.0" prefHeight="18.0" prefWidth="350.0" text="board construction isn't done " />
      <TextField fx:id="categoryName" layoutX="425.0" layoutY="72.0" prefHeight="26.0" prefWidth="137.0" promptText="category name" />
      <Button fx:id="addCategoryBtn" layoutX="425.0" layoutY="108.0" mnemonicParsing="false" onAction="#addCategory" prefHeight="26.0" prefWidth="138.0" text="add category" />
      <ScrollPane layoutX="4.0" layoutY="52.0" prefHeight="250.0" prefWidth="282.0">
         <content>
            <HBox fx:id="hBox" prefHeight="250.0" prefWidth="293.0" />
         </content>
      </ScrollPane>
      <Button fx:id="addTaskBtn" layoutX="425.0" layoutY="144.0" mnemonicParsing="false" onAction="#addTask" prefHeight="26.0" prefWidth="138.0" text="add task" />
      <Button layoutX="424.0" layoutY="177.0" mnemonicParsing="false" onAction="#gotoFailedTasksPage" prefHeight="26.0" prefWidth="138.0" text="FailedTasks" />
      <Label fx:id="Completion" layoutX="14.0" layoutY="326.0" prefHeight="18.0" prefWidth="350.0" text="board completion percentage : " />
   </children>
   <stylesheets>
      <URL value="@../CSS/style.css"/>
   </stylesheets>
</AnchorPane>
